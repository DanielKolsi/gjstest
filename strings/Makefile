PROJECT_ROOT = ..
default: strings.a

# Tools and flags.
include $(PROJECT_ROOT)/tools.mk

# Objects to include in the library.
OBJS = ascii_ctype.o strutil.o

strings.a : $(OBJS)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $^

# depend target
include $(PROJECT_ROOT)/depend_target.mk

clean :
	rm -f *.o
	rm -f *.a

