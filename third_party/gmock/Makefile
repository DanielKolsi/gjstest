PROJECT_ROOT = ../..
default: gmock_main.a

# Tools and flags.
#
# TODO(jacobsa): Instead of mucking with the include path, update the includes
# in the foo-all.cc files.
include $(PROJECT_ROOT)/tools.mk
CPPFLAGS += -I. -I../gtest

# Objects to include in the library.
OBJS = src/gmock-all.o ../gtest/src/gtest-all.o src/gmock_main.o

gmock_main.a : $(OBJS)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $^

# depend target
include $(PROJECT_ROOT)/depend_target.mk

clean :
	rm -f *.o
	rm -f *.a

