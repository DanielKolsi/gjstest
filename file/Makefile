PROJECT_ROOT = ..
default: file.a

# Tools and flags.
include $(PROJECT_ROOT)/tools.mk

# Objects to include in the library.
OBJS = file_utils.cc

strings.a : $(OBJS)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $^

# depend target
include $(PROJECT_ROOT)/depend_target.mk

clean :
	rm -f *.o
	rm -f *.a

