PROJECT_ROOT = ../..
default: xml.a

# Tools and flags.
include $(PROJECT_ROOT)/tools.mk
CPPFLAGS += -I/usr/include/libxml2

# Objects to include in the library.
OBJS = xml_writer.o

xml.a : $(OBJS)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $^

# depend target
include $(PROJECT_ROOT)/depend_target.mk

clean :
	rm -f *.o
	rm -f *.a

