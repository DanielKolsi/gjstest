PROJECT_ROOT = ../..
default: cityhash.a

# Tools and flags.
include $(PROJECT_ROOT)/tools.mk

# Objects to include in the library.
OBJS = city.o

cityhash.a : $(OBJS)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $^

# depend target
include $(PROJECT_ROOT)/depend_target.mk

clean :
	rm -f *.o
	rm -f *.a

## Dependencies: (autogenerated) ##
city.o: city.cc ../../third_party/cityhash/city.h
